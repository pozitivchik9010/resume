<main>
    {{#with heading}}
    {{#with main}}


    <div class="card">
        <div class="card-body">

            <div class="card">
                <div class="card-body">
                    <h1 class="text-capitalize">{{title}} </h1>

                    <a>{{text}}</a>
                    <br>
                    <br>
                    {{#with button_text}}

                    <a href="#" class="btn btn-primary">
                        {{solid}}

                    </a>
                    <br>
                    <br>
                    <br>
                    <button class=" btn btn-outline-primary">{{outline}}</button>

                    {{/with}}
                </div>
            </div>
            <br>
            {{/with}}
            {{#with second}}
            <div class="card">
                <div class="card-body">
                    <h2 class="text-capitalize">{{title}} </h2>

                    <a>{{text}}</a>
                    <br>
                    <br>
                    {{#with button_text}}

                    <a href="#" class="btn btn-secondary">
                        {{solid}}

                    </a>
                    <br>
                    <br>
                    <br>
                    <button class=" btn btn-outline-secondary">{{outline}}</button>

                    {{/with}}
                </div>
            </div>
            {{/with}}
            <br>
            {{#with subblock}}
            <div class="card">
                <div class="card-body">
                    <h3 class="text-capitalize">{{title}} </h3>

                    <a>{{text}}</a>
                    <br>
                    <br>
                    {{#with button_text}}

                    <a href="#" class="btn btn-dark">
                        {{solid}}

                    </a>
                    <br>
                    <br>
                    <br>
                    <button class=" btn btn-outline-dark">{{outline}}</button>

                    {{/with}}
                </div>
            </div>
            {{/with}}

        </div>
    </div>
    {{/with}}
    <br>
    {{#with nested_display}}

    <div class="card">
        <div class="card-body">
            <div class="display-1 text-dark">{{title}} </div>
            <br>
            {{#with children_display}}
            <div class="card">
                <div class="card-body">
                    <div class="display-4 text-dark">{{title}} </div>
                    <br>
                    <a>{{text}}</a>
                    <br>
                    <br>
                    <a href="#" class="btn btn-outline-warning">
                        {{button/text}}
                    </a>
                </div>
            </div>
            {{/with}}
            <br>
            <h3 class="text-capitalize">{{header_text}} </h3>
            <br>
            <button class=" btn btn-info">{{button/text}}</button>
        </div>
    </div>
    {{/with}}
    <br>
    {{#with paragraph}}
    <div class="card">
        <div class="card-body">
            <div class=" text-uppercase">{{title}} </div>
            <br>
            <div class="text-lowercase">{{text}} </div>
            <br>
            {{#with button}}
            <a href="{{url}}" class="btn btn-link">
                {{text}}
            </a>
            {{/with}}
            <br>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#each list_name}}
                    <ul>
                        <li>
                            <a href="{{url}}" class="btn btn-link" target="_blank">
                                {{text}}
                            </a>
                        </li>
                    </ul>
                    {{/each}}
                </div>

            </div>
        </div>
    </div>
    {{/with}}

</main>